<h1>New <%= model.camelize %></h1>

<% if !namespace.blank? %>
<%% form_for([:<%= namespace %>, @<%= singular_name %>]) do |f| %>
<% else %>
<%% form_for(@<%= singular_name %>) do |f| %>
<% end %>
  <%%= render :partial => 'form', :locals => {:f => f} %>
  <p>
    <%%= f.submit "Create" %>
  </p>
<%% end %>

<%%= link_to 'Back', <%= plural_name %>_path %>
